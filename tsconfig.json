{
  "compilerOptions": {
    "jsxImportSource": "astro/jsx",
    "allowJs": true,
    "module": "ES2020",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}